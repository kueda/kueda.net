@misc{california_native_plant_society_california_2025,
	title = {California {Fuchsia}},
	url = {https://calscape.org/Epilobium-canum-(California-Fuchsia)},
	abstract = {California Fuchsia (Epilobium canum) is a beautiful species of willowherb, native to the California foothills and coastal areas. It is a perennial plant, notable for the profusion of bright scarlet flowers in summer and autumn --- it's frequently the only native California plant in an area flowering at the height of summer. They tend to die back and go dormant in the winter. {\textless}br{\textgreater}{\textless}br{\textgreater}Other common names include California Fuchsia (from the resemblance of the flowers to those of Fuchsias), Hummingbird Flower, and Hummingbird Trumpet (as the flowers are very attractive to hummingbirds). {\textless}br{\textgreater}{\textless}br{\textgreater}California Fuchsia is often found by seasonal creeks, seeps and spring, particularly in the drier southern part of its range.{\textless}br{\textgreater}{\textless}br{\textgreater}California Fuchsia is easy to grow. It does best and will flower most profusely in full sun. In the wetter, northern part of its range or near the coast, this plant will typically require no supplemental water after established. In the drier, hotter, inland southern areas, it will often die without summer water unless planted close to an irrigated or other wet area. You can water it 1x/month without much danger. {\textless}br{\textgreater}{\textless}br{\textgreater}Plants tend to get straggly after flowering by late fall or early winter. Best to cut it back to the ground as soon as the flowers are spent, and it will come back back lush and healthy in the spring. Otherwise, it will look straggly and unhealthy the next year and is more likely to die. {\textless}br{\textgreater}{\textless}br{\textgreater}This plant will readily self-seed, so once you get this species established, it will usually start springing up around your garden. It also spreads by rhizomes. There's probably no better California native plant for attracting hummingbirds.{\textless}br{\textgreater}{\textless}br{\textgreater}This plant is on several fire-resistant plant lists, including FireSafe Marin and County of San Diego.},
	language = {en},
	urldate = {2025-03-21},
	journal = {Calscape},
	author = {{California Native Plant Society}},
	year = {2025},
	file = {Snapshot:/Users/kueda/Zotero/storage/UM5RFAVE/Epilobium-canum-(California-Fuchsia).html:text/html},
}

